{
    "name": "psliwa/pdf-bundle",
    "description": "This bundle integrates Symfony2 with PHPPdf library.",
    "keywords": ["PDF", "PHPPdf"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name":     "Piotr Åšliwa",
            "homepage": "http://psliwa.org",
            "email":    "me@psliwa.org"
        }     
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/teamshirtsde/PHPPdf.git"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "doctrine/annotations": "^1.0",
        "symfony/config": "~4.0|~5.0",
        "symfony/dependency-injection": "~4.0|~5.0",
        "symfony/framework-bundle": "~4.0|~5.0",
        "symfony/http-foundation": "~4.0|~5.0",
        "symfony/http-kernel": "~4.0|~5.0",
        "symfony/templating": "~4.0|~5.0",
        "psliwa/php-pdf": "dev-teamshirts",
        "sensio/framework-extra-bundle": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4,<6.0.0"
    },
    "config": {
        "preferred-install": {
            "psliwa/*": "source",
            "*": "auto"
        }
    },
    "autoload": {
        "psr-0": { "Ps\\PdfBundle": "" }
    },
    "target-dir": "Ps/PdfBundle"
}
